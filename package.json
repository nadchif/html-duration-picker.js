{
  "name": "html-duration-picker",
  "version": "2.0.1",
  "description": "Turn an html input box to a duration picker, without jQuery",
  "main": "dist/html-duration-picker.min.js",
  "types": "dist/html-duration-picker.min.d.ts",
  "scripts": {
    "test": "eslint src/html-duration-picker.js && jasmine",
    "test-eslint": "eslint src/html-duration-picker.js",
    "test-jasmine": "jasmine",
    "build": "webpack --mode=production --progress --colors",
    "dev": "lite-server --baseDir=\"src\"",
    "buildpatch": "npm version patch && eslint src/html-duration-picker.js && webpack --mode=production",
    "deploy": "npm run build && npm publish"
  },
  "keywords": [
    "html5",
    "picker",
    "native",
    "input"
  ],
  "author": "Chif (nadchif@gmail.com)",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/nadchif/html-duration-picker.js.git"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "babel-loader": "^8.0.6",
    "create-file-webpack": "^1.0.2",
    "eslint": "^6.7.2",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-jasmine": "^4.1.0",
    "jasmine": "^3.5.0",
    "jasmine-core": "^3.5.0",
    "jsdom": "^15.2.1",
    "lite-server": "^2.5.4",
    "path": "^0.12.7",
    "webpack": "^4.41.2",
    "webpack-auto-inject-version": "^1.2.2",
    "webpack-cli": "^3.3.10"
  }
}
